#Date: 27/Jun/2018
#Description: Test program for BEQ, BNE

.text
	addi $t0, $zero, 5
	add $t1, $zero, $zero
	while:
	beq $t1, $t0, exit
	addi $t1, $t1, 1
	bne $t1, $zero, while
	
	exit:
	add $t2, $t1, $t1
	sll $t3, $t2, 2
	
	addi $t2, $zero, 0x10010000
	addi $t0, $zero, 5
	sw $t0, 0($t2)
	lw $t1, 0($t2)
